{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for react app",
  "scripts": {
    "lint": "npm run lint:js",
    "lint:js": "eslint . --ignore-path .gitignore --ignore-pattern internals/scripts",
    "start": "NODE_ENV=development node server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidebettio/react-boilerplate.git"
  },
  "author": "Davide Bettio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidebettio/react-boilerplate/issues"
  },
  "homepage": "https://github.com/davidebettio/react-boilerplate#readme",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "max-len": 0
    }
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "express": "^4.14.0",
    "ip": "^1.1.3",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.1",
    "html-webpack-plugin": "^2.21.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1"
  }
}
